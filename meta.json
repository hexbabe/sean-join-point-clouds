{
  "module_id": "viam:join_point_clouds",
  "visibility": "public",
  "url": "https://github.com/viam-modules/join-point-clouds",
  "description": "Viam C++ Module to join multiple point cloud sources from one perspective",
  "models": [
    {
      "api": "rdk:component:camera",
      "model": "viam:join-camera:join-point-clouds"
